\begin{frame}{Проблема синтеза для дискретных систем}
\begin{block}{}
    \begin{gather}
        J(t_0,x_0,u(\cdot))=\int_{t_0}^{T}f^0(x(\tau),u(\tau),\tau)d\tau + \varphi (x(T)) \rightarrow inf, \\
        \overset{.}{x}=f(x(\tau),u(\tau),\tau), \; t_0 \leqslant \tau \leqslant T, \; x(t_0)=x_0, \\
        x(\tau) \in G(\tau), \; t_0 \leqslant  \tau \leqslant T,\\
        u=u(\tau \in V(\tau), \;  t_0 \leqslant  \tau \leqslant T; \; u(\tau) - \text{кусочно-непрерывна;}
    \end{gather}
    \end{block}
\end{frame}

\begin{frame}{Проблема синтеза для дискретных систем}
    \begin{block}{}
        \begin{gather}
           % \begin{split}
                I_0 (x;[u_i]_0 = \sum_{i=0}^{N-1}F_i^0(x_i,u_i)+\varphi(x_N) \rightarrow inf, \\
                x_{i+1} = F_i(x_i,u_i), \; i=0, \dots ,N-1, \; \; x_0=x,\\
                x_i \in G_i, \; i=0, \dots , N,\\
                [u_i]_0=(u_0,u_1, \dots ,u_{N-1}):u_i \in V_i, \; i=0. \dots ,N-1
          %  \end{split}
        \end{gather}
    \end{block}
    \[
        \begin{split}
            F_{i}^{0}(x,u) &= f^0(x,u,t_i)(t_{i+1} - t_i), \\
            F_i (x,u) &= x + f(x,u,t)(t_{i+1} - t_i), \\
            G_i &= G(t_i), \; V_i=V(t_i)
        \end{split}
    \]
\end{frame}

\begin{frame}{Проблема синтеза для дискретных систем}
    \begin{block}{}
        \begin{gather}
            I_k(x,[u_i]_k) = \sum_{i=k}^{N-1}F_{i}^{0}(x_i,u_i) + \Phi(x_N) \rightarrow inf, \\
            x_{i+1} = F_i (x_i,u_i), \; i=k, \dots ,N-1, \; x_k=x, \\
            x_i \in G_i, \; \; i=k, \dots ,N, \\
            [u_i]_k=(u_k,u_{k+1}, \dots , u_{N-1}):u_i \in V_i, \; i=k, \dots ,N-1
        \end{gather}
    \end{block}
\end{frame}

\begin{frame}{Проблема синтеза для дискретных систем}
    Введем $B_k(x)=\underset{\Delta_k(x)}{\inf} I_k(x,[u_i]_k), \; k=0, \dots , N-1$
   % \newtheorem{Th}{Теорема}
    \begin{Th}[]\label{th2}
        Функция Беллмана задачи (18)-(21) необходимо является решением уравнения
    \small{
        \begin{gather}
            B_k(x)= \underset{u \in D_k(x)}{\inf}[F_{k}^{0}(x,u) + B_{k+1}(F_k(x,u))], \; x \in X_k, k=0, \dots ,N-1
            \intertext{где}
            B_N(x)= \Phi(x), \; x \in G_N,
        \end{gather}
    }
        $D_k(x)$ - множество всех тех $u \in V_k,$ для которых существует хотя бы одно управление $[u_i]_k \in \Delta_k(x)$ с компонентой $u_k = u.$ \\
        Верно и обратное: функция $B_k(x), x \in X_k, k=0, \dots , N-1,$ определяемая условиями (22),(23), является функцией Беллмана задачи (14)-(17).
    \end{Th}
\end{frame}

\begin{frame}{Проблема синтеза для дискретных систем}
    В теории оптимального управления и ее приложениях важное место занимает так называемая \textit{проблема синтеза}, заключащаяся в построении функции $u=u_k(x)$, выражающей собой оптимальное управление при условии, то в момент $k$ объект находится в точке $x$ фазового пространства. Такая функция $u_k(x)$ называется \textit{синтезирующей}.
\end{frame}

\begin{frame}{Проблема синтеза для дискретных систем}
    \begin{Th}[]\label{th3}
    Пусть множества $G_k, k=0, \dots ,N,$ замкнуты, множества $V_k, k=0, \dots ,N-1$, замкнуты и ограничены, функция $F_{k}^{0}(x,u)$ полунепрерывна снизу ... (стр 3)
    \end{Th}
\end{frame}

